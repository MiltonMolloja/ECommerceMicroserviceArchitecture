{
  "SecretKey": "${JWT_SECRET_KEY}",
  "DatabaseLogging": {
    "Enabled": false
  },
  "ApiUrls": {
    "IdentityUrl": "${IDENTITY_SERVICE_URL}",
    "CatalogUrl": "${CATALOG_SERVICE_URL}",
    "CustomerUrl": "${CUSTOMER_SERVICE_URL}",
    "OrderUrl": "${ORDER_SERVICE_URL}",
    "CartUrl": "${CART_SERVICE_URL}",
    "PaymentUrl": "${PAYMENT_SERVICE_URL}"
  },
  "Redis": {
    "ConnectionString": "${REDIS_CONNECTION_STRING}",
    "InstanceName": "Gateway_"
  },
  "CacheSettings": {
    "CacheExpirationMinutes": 5,
    "Disabled": false
  },
  "ApiKey": {
    "EnableApiKeyValidation": true,
    "ApiKey": "${API_KEY}"
  },
  "Logging": {
    "IncludeScopes": true,
    "LogLevel": {
      "Default": "Warning",
      "System": "Warning",
      "Microsoft": "Warning"
    }
  },
  "AllowedHosts": "${ALLOWED_HOSTS}"
}
